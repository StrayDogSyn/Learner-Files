    "build": "tsc -b && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "check": "tsc --noEmit",
    "test": "echo \"No tests specified\" && exit 0",
    "dev": "npm run client:dev",
    "predeploy": "npm run lint && npm run check && npm run test && npm run build",
    "deploy": "npm run build && gh-pages -d dist -b gh-pages",
    "deploy:force": "npm run build && gh-pages -d dist -b gh-pages -f",
    "analyze": "npx vite-bundle-analyzer dist/stats.html",
    "clean": "rm -rf dist node_modules/.vite",
    "reinstall": "npm run clean && npm install"
